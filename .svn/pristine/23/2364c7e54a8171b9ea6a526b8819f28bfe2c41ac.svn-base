#import "FontUtil.h"

@implementation FontUtil
+(UIFont *)boldSystemFontOfSize:(CGFloat)size{
    UIFont *font = [UIFont fontWithName:@"TazuganeGothicStdN-Bold" size:size];
    return font;
}
+(UIFont *)systemFontOfSize:(CGFloat)size{
    UIFont *font = [UIFont fontWithName:@"TazuganeGothicStdN-Light" size:size];
    return font;
}
+(UIFont *)systemFontOfSizeMedium:(CGFloat)size{
    UIFont *font = [UIFont fontWithName:@"TazuganeGothicStdN-Medium" size:size];
    return font;
}
@end
